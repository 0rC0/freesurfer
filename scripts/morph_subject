set version = '$Id: morph_subject,v 1.3 2007/01/06 00:01:14 nicks Exp $';

if($#argv != 1) then
  echo "USAGE: morph_subject subjid"
  echo $version
  exit 1;
endif

set subjdir =  $SUBJECTS_DIR/$1
if(! -e $subjdir) then
  echo "ERROR: $subjdir does not exist"
  exit 1;
endif

morph_subject-lh $1
morph_subject-rh $1

exit 0;
